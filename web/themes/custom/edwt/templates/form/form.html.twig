{#
/**
 * @file
 * Eau de Web Theme implementation for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 *
 * @ingroup themeable
 */
#}

{{ attach_library('edwt/bc--forms') }}

{% block form %}
  <form{{ attributes }}>
    {{ children }}
  </form>
{% endblock %}
