{#
/**
 * @file
 * WWD override for a field.
 */
#}

{%
  set classes = [
    'field',
    'field--name-' ~ field_name|clean_class,
  ]
%}

<div{{ attributes.addClass(classes) }}>
  {% for item in items %}
    {{ item.content.0|length > 129 ? item.content.0|slice(0, 129) ~ '...' : item.content  }}
  {% endfor %}
</div>
