{#
/**
 * @file
 * WWD override to display a node.
 */
#}

{{ attach_library('wwd/n--partners--teaser') }}

{%
  set classes = [
    'node',
    'node--' ~ node.bundle|clean_class,
    'node--' ~ node.bundle|clean_class ~ '--' ~ view_mode|clean_class,
  ]
%}

<article{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {{ title_suffix }}
  {{ content }}
</article>
